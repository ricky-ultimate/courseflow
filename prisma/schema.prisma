datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

model Complaint {
  id            Int      @id @default(autoincrement())
  name          String
  department    String
  email         String
  message       String
  createdAt     DateTime @default(now())
}
