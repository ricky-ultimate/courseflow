datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Complaint {
  id            Int      @id @default(autoincrement())
  name          String
  department    String
  email         String
  message       String
  createdAt     DateTime @default(now())
  status        String   @default("unresolved")
}
